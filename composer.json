{
    "name": "pagofacil/domain-php",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-curl": "*",
        "ramsey/uuid": "3.8.*",
        "league/fractal": "0.18.*",
        "guzzlehttp/guzzle": "6.4.*",
        "psr/http-client": "1.0.*",
        "psr/http-message": "1.0.*",
        "psr/log": "1.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "8.4.*",
        "friendsofphp/php-cs-fixer": "2.16.*",
        "phploc/phploc": "5.0.*",
        "phpmetrics/phpmetrics": "2.4.*",
        "phpstan/phpstan": "0.11.*",
        "phpstan/phpstan-phpunit": "0.11.*",
        "squizlabs/php_codesniffer": "3.5.*",
        "phpmd/phpmd": "2.7.*",
        "monolog/monolog": "2.0.*"
    },
    "autoload": {
        "psr-4": {
            "PagoFacil\\Gateway\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PagoFacil\\Test\\": "test/src/"
        }
    }
}